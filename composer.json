{
    "name": "cobisja/spn",
    "description": "Simple classes for sending Push Notifications via Firebase Cloud Messaging",
    "keywords": ["laravel", "fcm", "dgm", "push notification", "device group manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cobis",
            "email": "jcobis@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": ">=5.2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "cobisja\\Spn\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "stable"
}
